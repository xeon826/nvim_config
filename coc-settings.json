{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "pylsp.builtin.installExtrasArgs": [
    "pydocstyle",
    "pycodestyle",
    "rope",
    "black"
  ],
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.config/coc/extensions/@yaegassy/coc-pylsp-data/pylsp/venv/bin/black",
  "coc.preferences.formatOnSaveFiletypes": [],
  "pylsp.builtin.enableInstallPylsIsort": true,
  "pylsp.builtin.enableInstallPythonLspBlack": true,
  "languageserver.python.settings.pyls.plugins.pylint_lint.enabled": false
}
