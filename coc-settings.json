{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
    "python": {
        "command": "jedi-language-server",
        "filetypes": ["python"]
    }
  },
  "python.formatting.provider": "autopep8"
}
