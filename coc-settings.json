{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "pylsp.builtin.installExtrasArgs": [
    "black",
  ],
  "inlayHint.enable": true,
  "pylsp.builtin.enableInstallPylsIsort": true,
  "pylsp.plugins.autopep8.enabled": false,
  "pylsp.builtin.enableInstallPythonLspBlack": true,
  "pylsp.builtin.enableInstallPythonLspRuff": false,
  "pylsp.builtin.pythonLspBlackVersion": "1.3.0",
}
